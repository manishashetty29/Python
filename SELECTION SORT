def selection(element):
    l=len(element)
    for i in range(l):
        min_index = i
        for j in range(i+1,l):
            if element[j] < element[min_index]:
                min_index=j
                element[i], element[min_index]= element[min_index],element[i]
    return element
element=[10,12,2,5,7,100,87]
res=selection(element)
print(res)
